INSERT INTO grocery_store (item, aisle, quantity, price) VALUES ("pasta sauce", 5, 3, 4.50);

--display the data set ordered by price
SELECT * FROM grocery_store ORDER BY "price" desc;

--what is the average price of the items in this grocery store?
SELECT AVG("price") FROM grocery_store;
