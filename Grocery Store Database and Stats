--Create a grocery store database

CREATE TABLE grocery_store (id INTEGER PRIMARY KEY, item TEXT, aisle INTEGER, quantity INTEGER, price INTEGER);

INSERT INTO grocery_store (item, aisle, quantity, price) VALUES ("bread", 3, 5, 3.99);
